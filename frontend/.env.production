# CosmoNFT Frontend - Production Environment Variables
# Copy this to .env.local for production

# ======================
# API
# ======================
# API routes are now part of the same Next.js app, no external URL needed
# NEXT_PUBLIC_API_URL is intentionally not set - uses relative URLs

# ======================
# STRIPE (Publishable Key)
# ======================
NEXT_PUBLIC_STRIPE_KEY=pk_live_YOUR_PUBLISHABLE_KEY

# ======================
# BLOCKCHAIN
# ======================
NEXT_PUBLIC_CONTRACT_ADDRESS=0xYOUR_DEPLOYED_CONTRACT_ADDRESS
NEXT_PUBLIC_CHAIN_ID=137

